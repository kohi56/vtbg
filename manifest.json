{
  "manifest_version": 3,
  "name": "Sheeter",
  "description": "Replace background with a custom image",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://app.voltaic.gg/*",
        "https://evxl.app/*"
      ],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["bg.jpg"],
      "matches": ["<all_urls>"]
    }
  ]
}